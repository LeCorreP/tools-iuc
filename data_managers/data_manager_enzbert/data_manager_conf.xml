<data_managers>
    <data_manager tool_file="data_manager/enzbert.xml" id="enzbert" version="1.0">
	<data_table name="enzbert">
	    <output>
		<column name="value"/>
		<column name="name"/>
		<column name="path" output_ref="output_file">
		    <move type="directory">
		        <source>${path}</source>
				<target base="${GALAXY_DATA_MANAGER_DATA_PATH}">enzbert/${value}</target>
		    </move>
			<value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/enzbert/${value}</value_translation>
		    <value_translation type="function">abspath</value_translation>
		</column>
	    </output>
	</data_table>
    </data_manager>
</data_managers>